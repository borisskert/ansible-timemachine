[Global]
mimic model = AirPort
log file = /dev/stdout
{% if log_level is defined %}
log level = default:{{log_level}}
{% else %}
log level = default:info
{% endif %}
zeroconf = no
# TODO: monitor transfer speed changes
dsireadbuf = 24

[Time Machine]
path = /timemachine
valid users = tmuser
time machine = yes
{% if volume_size_limit is defined %}
vol size limit = {{volume_size_limit}}
{% endif %}
